{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build/unbundled",
    "rewrites": [
      {
        "source": "/.well-known/assetlinks.json",
        "headers" : [
          {
            "key" : "Content-Type",
            "value" : "application/json"
          }
        ],
        "type" : 200
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
